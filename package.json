{
  "name": "hci-ng-grid",
  "version": "4.0.0-beta.0",
  "description": "Comprehensive angular grid component",
  "keywords": [
    "angular",
    "angular2",
    "data",
    "grid",
    "list",
    "table"
  ],
  "license": "MIT",
  "repository": "git+https://github.com/HuntsmanCancerInstitute-RISR/hci-ng-grid.git",
  "homepage": "https://huntsmancancerinstitute-risr.github.io/hci-ng-grid-demo",
  "scripts": {
    "build": "npm run clean && npm run ngc && npm run doc && npm run karma start && npm run copyfiles package.json dist/ && npm run copyfiles -- src/grid/themes/*.css dist/",
    "clean": "npm run rimraf index.js index.d.ts index.ngsummary.json index.metadata.json index.js.map src/**/*.js src/**/*.ngfactory.ts src/**/*.d.ts src/**/*.js.map src/**/*.ngsummary.json src/**/*.metadata.json code-coverage dist",
    "compodoc": "compodoc",
    "copyfiles": "copyfiles",
    "copyToDist": "npm run copyfiles -- LICENSE package.json README.md dist/",
    "doc": "npm run compodoc -- src -p ./tsconfig.json -d docs -n 'hci-ng-grid'",
    "karma": "karma",
    "ngc": "ngc",
    "npmPack": "npm run build && npm run copyToDist && cd dist && npm pack",
    "npmPublish": "npm run build && npm run copyToDist && cd dist && npm publish --access public",
    "push": "cd dist && npm run copyfiles -- \"index.+(js|d.ts|js.map|metadata.json)\" ../demo/node_modules/hci-ng-grid && npm run copyfiles -- -e \"*.+(ngfactory|ngsummary).*\" \"src/**/*.+(js|d.ts|js.map|metadata.json|css)\" ../demo/node_modules/hci-ng-grid && cd ..",
    "rimraf": "rimraf"
  },
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.+(js|d.ts|js.map|metadata.json)",
    "src/**/!(*spec).+(js|d.ts|js.map|metadata.json|css)"
  ],
  "dependencies": {
    "@ng-bootstrap/ng-bootstrap": "1.1.x",
    "moment": ">=2.19.3"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@compodoc/compodoc": "1.0.x",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.0",
    "@types/webpack": "4.1.2",
    "angular2-cool-storage": "3.1.5",
    "awesome-typescript-loader": "^3.2.3",
    "angular2-template-loader": "^0.6.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "cryptiles": ">=4.1.2",
    "css-loader": "^0.26.0",
    "del": "^2.2.1",
    "deep-extend": ">=0.5.1",
    "event-stream": "3.3.4",
    "fs": "^0.0.2",
    "immutable": "^3.8.1",
    "inline-source-map": "^0.6.2",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "macaddress": ">=0.2.9",
    "minimist": "^1.2.0",
    "reflect-metadata": "^0.1.3",
    "rimraf": "^2.6.1",
    "rxjs": "^5.5.5",
    "semver": "5.6.x",
    "source-map-loader": "^0.2.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.5.1",
    "typescript": "^2.5.1",
    "url-parse": ">=1.4.3",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.12"
  }
}
