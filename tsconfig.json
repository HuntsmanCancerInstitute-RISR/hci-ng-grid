{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noEmitHelpers": false,
    "declaration": true,
    "outDir": "./",
    "types": [ "node", "jasmine", "systemjs" ]
  },
  "exclude": [
    "node_modules",
    "**/*.d.ts"
  ]
}
