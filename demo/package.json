{
  "name": "hci-ng-grid-demo",
  "description": "A demo application for hci-ng-grid component development",
  "private": true,
  "scripts": {
    "build": "npm run build:server",
    "build:live": "npm run clean && npm run ngc && npm run webpack -- --config webpack/webpack.live.js",
    "build:server": "npm run clean && npm run ngc && npm run webpack -- --env.production --config webpack/webpack.server.js",
    "clean": "npm run rimraf src/**/*.js src/**/*.js.map src/**/*.ngstyle.ts src/**/*.ngfactory.ts src/**/*.ngsummary.json src/**/*.metadata.json src/**/*.d.ts build dist",
    "ngc": "ngc",
    "rimraf": "rimraf",
    "serve": "node server.js",
    "run-demo": "npm run clean && npm run ngc && webpack-dev-server --config webpack/webpack.live.js",
    "run-demo-prod": "npm run build:server && npm run serve",
    "webpack": "webpack"
  },
  "optionalDependencies": {
    "hci-ng-grid": "5.3.x"
  },
  "dependencies": {
    "@angular/cdk": ">=8.0.0 <10.0.0",
