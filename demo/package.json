{
  "name": "hci-ng-grid-demo",
  "description": "A demo application for hci-ng-grid component development",
  "private": true,
  "scripts": {
    "build": "npm run build:server",
    "build:live": "npm run clean && npm run ngc && npm run webpack -- --config webpack/webpack.live.js",
    "build:server": "npm run clean && npm run ngc && npm run webpack -- --config webpack/webpack.server.js",
    "clean": "npm run rimraf src/**/*.js src/**/*.js.map src/**/*.ngstyle.ts src/**/*.ngfactory.ts src/**/*.ngsummary.json src/**/*.metadata.json src/**/*.d.ts build dist",
    "ngc": "ngc",
    "postinstall": "cd .. && npm run build && npm run push && cd demo",
    "rimraf": "rimraf",
    "serve": "node server.js",
    "start": "npm run clean && npm run ngc && webpack-dev-server --config webpack/webpack.live.js",
    "start-prod": "npm run build:server && npm run serve",
    "webpack": "webpack"
  },
  "optionalDependencies": {
    "hci-ng-grid": "2.0.0-beta.2"
  },
  "dependencies": {
    "@angular/common": ">5.2.0 <= 5.2.10",
    "@angular/compiler": ">5.2.0 <= 5.2.10",
    "@angular/compiler-cli": ">5.2.0 <= 5.2.10",
    "@angular/core": ">5.2.0 <= 5.2.10",
    "@angular/forms": ">5.2.0 <= 5.2.10",
    "@angular/http": ">5.2.0 <= 5.2.10",
    "@angular/platform-browser": ">5.2.0 <= 5.2.10",
    "@angular/router": ">5.2.0 <= 5.2.10",
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/fontawesome-free-regular": "^5.0.4",
    "@fortawesome/fontawesome-free-solid": "^5.0.4",
    "@ng-bootstrap/ng-bootstrap": "1.0.4",
    "bootstrap": "^4.0.0",
    "moment": "2.15.1",
    "moment-random": "^1.0.3",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.42",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^7.0.0",
    "@types/selenium-webdriver": "~2.53.39",
    "@types/source-map": "^0.5.0",
    "@types/webpack": "4.1.2",
    "angular-router-loader": "^0.6.0",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "3.1.2",
    "base-href-webpack-plugin": "^1.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.0",
    "fs": "^0.0.2",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "node-sass": "4.7.2",
    "postcss-loader": "1.3.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.3",
